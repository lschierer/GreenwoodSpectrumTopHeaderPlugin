{
  "name": "topheader-plugin",
  "private": true,
  "version": "1.0.1",
  "description": "A plugin for Greenwood that adds a top header component",
  "type": "module",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "types": "./dist/plugins/TopHeaderSectionPlugin.d.ts",
      "import": "./dist/plugins/TopHeaderSectionPlugin.js",
      "default": "./dist/plugins/TopHeaderSectionPlugin.js"
    },
    "./components/*": "./dist/components/*",
    "./styles/*": "./dist/styles/*"
  },
  "main": "./dist/plugins/TopHeaderSectionPlugin.js",
  "types": "./dist/plugins/TopHeaderSectionPlugin.d.ts",
  "scripts": {
    "prepare": "rm -rf dist/ && mkdir -p dist/ && rsync -rv --exclude 'pages/' src/ dist/ && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run prepare",
    "dev": "npm run prepare && NODE_OPTIONS='--experimental-strip-types --import @greenwood/cli/register' greenwood develop",
    "start": "npm run prepare && NODE_OPTIONS='--import @greenwood/cli/register' greenwood develop",
    "build:pre": "npm run prepare && mkdir -pv ./node_modules/topheader-plugin/dist && rsync -rv dist/ ./node_modules/topheader-plugin/dist",
    "build": "npm run build:pre && NODE_OPTIONS='--experimental-strip-types --import @greenwood/cli/register' greenwood build",
    "build:debug": "npm run build:pre && NODE_OPTIONS='--trace-warnings --inspect --experimental-strip-types --import @greenwood/cli/register' greenwood build",
    "serve": "NODE_OPTIONS='--experimental-strip-types --import @greenwood/cli/register' greenwood serve",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,css,md}\"",
    "link-local": "npm run prepare && pnpm link --global && cd node_modules && ln -sf ../dist topheader-plugin"
  },
  "devDependencies": {
    "@eslint/css": "^0.6.0",
    "@eslint/js": "^9.24.0",
    "@eslint/markdown": "^6.3.0",
    "@greenwood/cli": "~0.32.0",
    "@greenwood/plugin-import-raw": "^0.32.0",
    "@greenwood/plugin-postcss": "^0.32.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/hast": "^3.0.4",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "postcss-sorting": "^9.1.0",
    "prettier": "^3.5.3",
    "rehype-autolink-headings": "^7.1.0",
    "remark-alerts": "^0.0.1",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  },
  "license": "CC-BY-SA-4.0",
  "dependencies": {
    "@spectrum-css/actionbutton": "^7.1.3",
    "@spectrum-css/button": "^14.1.6",
    "@spectrum-css/icon": "^9.1.0",
    "@spectrum-css/link": "^7.1.0",
    "@spectrum-css/menu": "^9.2.0",
    "@spectrum-css/page": "^9.1.0",
    "@spectrum-css/picker": "^9.1.4",
    "@spectrum-css/sidenav": "^7.1.0",
    "@spectrum-css/tokens": "^16.0.1",
    "@spectrum-css/typography": "^8.1.0",
    "@spectrum-web-components/split-view": "^1.5.0",
    "@spectrum-web-components/theme": "^1.5.0",
    "github-base.css": "link:remark-alerts/styles/github-base.css",
    "github-colors-dark-class.css": "link:remark-alerts/styles/github-colors-dark-class.css",
    "github-colors-light.css": "link:remark-alerts/styles/github-colors-light.css",
    "iconify-icon": "^2.3.0",
    "rehype-parse": "^9.0.1",
    "rehype-stringify": "^10.0.1",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "zod": "^3.24.3"
  }
}
